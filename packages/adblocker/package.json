      "email": "arshadasif0312@gmail.com"
    },
    {
      "name": "Anton Lazarev",
      "email": "antonok35@gmail.com"
    }
  ],
  "devDependencies": {
    "@remusao/smaz-generate": "^1.9.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.0.2",
    "axios": "^1.7.2",
    "chai": "^5.1.0",
    "concurrently": "^8.2.1",
    "eslint": "^9.3.0",
    "mocha": "^10.2.0",
    "nyc": "^17.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.0.2",
    "tshy": "^3.0.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@cliqz/adblocker-content": "^1.31.2",
    "@cliqz/adblocker-extended-selectors": "^1.31.2",
    "@remusao/guess-url-type": "^1.3.0",
    "@remusao/small": "^1.2.1",
    "@remusao/smaz": "^1.9.1",
    "@types/chrome": "^0.0.269",
    "@types/firefox-webext-browser": "^120.0.0",
    "tldts-experimental": "^6.0.14"
  }
}
